<odoo>

  	<record id="pos_config_view_form_inherit_add_pricelist" model="ir.ui.view" >
		<field name="name">delivery.order.pos.config.form</field>
		<field name="model">pos.config</field>
		<field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
		<field name="arch" type="xml">
			<xpath expr="//div[@id='category_reference']" position="after">

				<div class="col-xs-12 col-md-6 o_setting_box">

					<div class="o_setting_left_pane">
						<field name="show_price_list"/>
					</div>
					<div class="o_setting_right_pane">
						<label for="show_price_list" string="Show Price Lists"/>
						<div class="text-muted">
							Show Price Lists
						</div>
					</div>
				</div>

			</xpath>
		</field>
	</record>


</odoo>
