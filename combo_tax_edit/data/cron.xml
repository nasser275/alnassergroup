<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="action_product_moves_transfer" model="ir.cron">
            <field name="model_id" ref="stock.model_stock_move_line"/>
            <field name="state">code</field>
            <field name="name">Modify Product Moves Transfers</field>
            <field name="active" eval="True"/>
            <field name="code">model._get_transfer_location()</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="interval_number">3</field>
            <field name="doall" eval="False"/>
        </record>

         <record id="action_pos_modify_tax" model="ir.cron">
            <field name="model_id" ref="point_of_sale.model_pos_order"/>
            <field name="state">code</field>
            <field name="name">Modify Tax Lines Order</field>
            <field name="active" eval="True"/>
            <field name="code">model._modify_combo_tax()</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="interval_number">1</field>
            <field name="doall" eval="False"/>
        </record>

        <record id="action_combo_modify_price" model="ir.cron">
            <field name="model_id" ref="point_of_sale.model_pos_order"/>
            <field name="state">code</field>
            <field name="name">Modify Combo Price Order</field>
            <field name="active" eval="True"/>
            <field name="code">model._modify_combo_price()</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="interval_number">1</field>
            <field name="doall" eval="False"/>
        </record>



    </data>
</odoo>