    <t t-name="ScheduleAppOrderButton" owl="1">
        <div class='control-button'>
            <i class="fa fa-clock-o" />
            Schedule Order
        </div>
    </t>
    <t t-name="ScheduleAppOrderPopupWidget" owl="1">
        <div class="modal-dialog">
            <div class="popup popup-schedule" style="max-width:560px !important; height:760px !important;">
                <div class="header">
                    <p class="title">Schedule Order</p>
                </div>
                <div class="body">
                    <div class="checkboxes">
                        <label for="saturday"><input class="weekdays" value="false" type="checkbox" id="saturday"/><span>Saturday</span></label>
                        <label for="sunday"><input class="weekdays" value="false" type="checkbox" id="sunday"/><span>Sunday</span></label>
                        <label for="monday"><input class="weekdays" value="false" type="checkbox" id="monday"/><span>Monday</span></label>
                        <label for="tuesday"><input class="weekdays" value="false" type="checkbox" id="tuesday"/><span>Tuesday</span></label>
                        <label for="wednesday"><input class="weekdays" value="false" type="checkbox" id="wednesday"/><span>Wednesday</span></label>
                        <label for="thursday"><input class="weekdays" value="false" type="checkbox" id="thursday"/><span>Thursday</span></label>
                        <label for="friday"><input class="weekdays" value="false" type="checkbox" id="friday"/><span>Friday</span></label>
                    </div>
                    <div class='required-field' style="position: relative;">
                        <span class=''>Start Date</span>
                        <input class="schedule-start-date" style="width: 300px;" type="date" name="schedule_start_date" required="required" />
                    </div>
                    <div class='required-field' style="position: relative;">
                        <span class=''>End Date </span>
                      <input class="schedule-end-date" style="width: 300px;margin-left: 10px;" type="date" name="schedule_end_date" required="required" />
                    </div>
                    <div class='saturday_time' style="position: relative;display:none">
                        <span class=''>Saturday Time </span>
