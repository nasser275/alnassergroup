<odoo>
    <data>
        <!-- Inherit Form View to Modify it -->
        <record id="pos_config_form_inherit" model="ir.ui.view">
            <field name="name">pos.config.form.inherit</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
            <field name="arch" type="xml">

                <xpath expr="//div[@id='company']" position="after">
                <div  class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="deviceSerialNumber"/>
                                <div><field name="deviceSerialNumber" /></div>
                            </div>
                        </div>
                    <div  class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="activityCode"/>
                                <div><field name="activityCode" /></div>
                            </div>
                        </div>
                    <div  class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="branch"/>
                                <div><field name="branch" /></div>
                            </div>
                        </div>

                    <div  class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="require_customer"/>
                                <div><field name="require_customer" /></div>
                            </div>
                        </div>
                    <div  class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="l10n_eg_production_env"/>
                                <div><field name="l10n_eg_production_env" /></div>
                            </div>
                        </div>
                    <div  class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="l10n_eg_client_identifier"/>
                                <div><field name="l10n_eg_client_identifier" /></div>
                            </div>
                        </div>
                    <div  class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="l10n_eg_client_secret"/>
                                <div><field name="l10n_eg_client_secret" /></div>
                            </div>
                        </div>
                     <div  class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="l10n_eg_production_env"/>
                                <div><field name="l10n_eg_production_env" /></div>
                            </div>
                        </div>

                </xpath>

            </field>
        </record>
    </data>
</odoo>