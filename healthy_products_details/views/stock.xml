        <record id="view_picking_form7777890" model="ir.ui.view">
            <field name="name">stock.picking.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                 <xpath expr="//field[@name='move_ids_without_package']//tree//field[@name='product_id']" position="attributes">
                    <attribute name="domain">[('type', 'in', ['product', 'consu']),('supplier_id','in',parent.supplier_ids), '|', ('company_id', '=', False), ('company_id', '=', company_id)]</attribute>
                </xpath>
                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="supplier_ids" widget="many2many_tags" invisible="1"/>
                </xpath>
            </field>
        </record>
        <record id="view_stock_move_line_detailed_operation_tree_55200008" model="ir.ui.view">
            <field name="name">stock.move.line.operations.tree</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='product_id']" position="attributes">
                    <attribute name="domain">[('type', '!=', 'service'),('supplier_id','in',parent.supplier_ids), '|', ('company_id', '=', False), ('company_id', '=', company_id)]</attribute>
                </xpath>
            </field>
        </record>





    </data>
